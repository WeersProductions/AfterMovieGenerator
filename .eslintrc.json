{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "no-continue": "off",
    "no-underscore-dangle": "off",
    "no-use-before-define": "off",
    "no-unused-vars": "off",
    "no-plusplus": "off",
    "no-param-reassign": "off",
    "react/prop-types": "off"
  }
}
